{
  "name": "init-berry",
  "version": "0.2.0",
  "bin": "./bin/internal.js",
  "exports": {
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "bin": {
      "init-berry": "./dist/index.js"
    },
    "exports": {
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "rm -rf dist && tsup ./src/index.ts --format cjs,esm --dts --onSuccess \"node ../../../scripts/postBuild.js\"",
    "dev": "yarn run build --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^28",
    "@types/node": "^20.3.1",
    "babel-jest": "^28",
    "eslint": "^8.43.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.23.3",
    "jest": "^28",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsup": "^7.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@toss/utils": "^1.4.3",
    "clipanion": "^3.2.1",
    "execa": "^5",
    "fs-extra": "^11.1.1"
  },
  "access": "public"
}
